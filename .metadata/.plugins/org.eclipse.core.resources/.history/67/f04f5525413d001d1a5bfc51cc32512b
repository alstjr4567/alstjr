package class_;

//1인분
public class MemberDTO {
	private String name; //멤버변수
	private int age;
	private String phone;
	private String adress;
	
	public MemberDTO(String name,int age,String phone,String adress) {  //생성자
		this.name=name;
		this.age=age;
		this.phone=phone;
		this.adress=adress;
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getAddress() {
		return adress;
	}

	public void setAddress(String address) {
		this.adress = address;
	}
	
}
/*
숙제
*/