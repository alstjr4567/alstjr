package array;

import java.util.Arrays;

public class z {

	public static void main(String[] args) {
		int[] lotto = new int[6];

		for(int i=0; i<lotto.length; i++) {
			lotto[i]=(int)(Math.random()*45+1);
		}
		int i = 0;
		for(int j=0; j<i;j++ ) {
			if(lotto[i] == lotto[j])
			i--;
			break;
		}
		
		Arrays.sort(lotto);//정렬 오름차순
		for(int j : lotto) {
			System.out.println(j + " ");
		}
		
		System.out.println();
		
		
	}

}
