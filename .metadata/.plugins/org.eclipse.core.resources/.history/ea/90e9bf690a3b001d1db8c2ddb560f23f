package constructor;

class Hello{
	private String name;
	private int age;
	
	public Hello() { //void리턴타입 쓰면 안됨.
		System.out.println("기본 생성자");
		
	}
	public Hello(String name) {
		this.name=name;
	}
	public String getName() {
		return name;
	}
}

//-------
public class ConstructorMain {
		private void mian(String[] args) {
			Hello aa = new Hello();
			
		}
}
