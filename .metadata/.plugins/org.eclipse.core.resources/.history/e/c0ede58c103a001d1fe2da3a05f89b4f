package array;

public class SelectionSort {

	public static void main(String[] args) {
		int[] ar = {35,48,25,72,50};
		
		System.out.println("정렬 전: ");
		for(int data : ar) {
			System.out.println(data+" ");
		}
		System.out.println();
		
		//Arrays.sort(ar); //Arrays클래스
		
		System.out.println("정렬 후: ");
		for(int data : ar) {
			System.out.println(data+" ");
		}
		System.out.println();
		
	}
}
