package for_;

import java.util.Scanner;

public class For08 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		System.out.println("숫자 입력 :");
		int num = sc.nextInt(); //6
		
		for(int i=1;i<=num;i++) {
			System.out.println(i);
		}
		
