		if(num%2==0)System.out.println(1);
		if(num%3==0)System.out.println(1);
		if(num%4==0)System.out.println(1);
		if(num%5==0)System.out.println(1);
		if(num%6==0)System.out.println(1);
		
//		System.out.println("숫자 입력 :");
