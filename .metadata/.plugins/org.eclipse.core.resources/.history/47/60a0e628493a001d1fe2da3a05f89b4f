package method;

public class Sungjuk {
	private String name;
	private int age;

	int kor;
	int eng; // 멤버변수
	int math;
	int avg;
	int tot;
	int grade;
	
	//정수 3개담을수있는 자료형만들었다. class로 만든다.

	//합계를 리턴하는 sum 메서드
	//	int sum(int k,int e,int m){
	//		return k + eng + m;
	//	}

	int sum(){ // 매개변수
		return kor + eng + math;
	}

	int avg(){ // avg 정의
		//return (kor+eng+math)/3;
		return sum() / 3;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public int getKor() {
		return kor;
	}

	public void setKor(int kor) {
		this.kor = kor;
	}

	public int getEng() {
		return eng;
	}

	public void setEng(int eng) {
		this.eng = eng;
	}

	public int getMath() {
		return math;
	}

	public void setMath(int math) {
		this.math = math;
	}

	public int getAvg() {
		return avg;
	}

	public void setAvg(int avg) {
		this.avg = avg;
	}

	public int getTot() {
		return tot;
	}

	public void setTot(int tot) {
		this.tot = tot;
	}

	public int getGrade() {
		return grade;
	}

	public void setGrade(int grade) {
		this.grade = grade;
	}
}
