package 연습장;

public class 클래스 {

	public static void main(String[] args) {
		
		show();//show메서드 호출
		System.out.println("가나다");
		show();//show메서드 호출
		System.out.println("라마바");
		show();
		System.out.println("----------");
	}
	static void show() {//메서드 정의
		System.out.println("show메스드 정의 부분");
		System.out.println("즐거운 자바 공부");
	}
	static void add() {
		
	}

}
/*
 위치에 상관없이 메인메서드부터 먼저 시작이된다.
 
*/