package for_;

public class MultiFor02 {
	public static void main(String[] args) {
		int dan,i = 0;
		
		System.out.println("구구단 1-9단");
		for(i=2;i<=9;i++) {
			for(dan=1;dan<=9;dan++) {
				System.out.println(i+"*"+dan+"= "+(i*dan));
			}
			System.out.println("-------------------");
			System.out.println();
		}
	}
}
/*
2단~9단 구구단

2*1=2
2*2=4
.
.
2*9=18

3*1=3
3*2=4
.
.
3*9=18

*/